{
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@chakra-ui/core": ">=1.0.0-rc.3",
    "next": ">=9",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "chakra-next-link",
  "author": "Fran Zekan",
  "module": "dist/chakra-next-link.esm.js",
  "size-limit": [
    {
      "path": "dist/chakra-next-link.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chakra-next-link.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@chakra-ui/core": "^1.0.0-rc.3",
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.3.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
